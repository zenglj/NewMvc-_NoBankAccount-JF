                                                else if (vcrd.PayMode == 1)
                                                {
                                                    strPayMode = "ATM机取款";
                                                }
                                                else if (vcrd.PayMode == 5)
                                                {
                                                    strPayMode = "放弃领款";
                                                }
                                                else
                                                {
                                                    strPayMode = "现金领取";
                                                }
                                                typeName = strPayMode;
                                                i++;
                                                if (groupTypeName != typeName && i != 1)
