                                                typeName = vcrd.DType;
                                                groupTypeName = strPayMode;
                                                sumMoney = sumMoney + vcrdMoney;
                                                groupMoney = groupMoney + vcrdMoney;
                                            }
                                            <tr><td colspan="8"></td><td colspan="2">@groupTypeName</td><td class="text-right">@groupMoney</td><td></td></tr>

