@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ParamSetting</title>
    @{Html.RenderPartial("~/Views/Shared/_PartialHeadPage.cshtml");}
</head>
<body class="easyui-layout">
    <div data-options="region:'center',title:'查询条件'" style="padding:5px;">
        <div style="height:300px;">
            <input type="hidden" id="typeName" />

            <input type="hidden" id="typeCode" value="@ViewBag.TypeCode" />
            <table id="test" class="easyui-datagrid" toolbar="#tb"></table>

        </div>

        <div id="tb" style="padding:5px;height:auto">
            <div>
                <a href="#" class="easyui-linkbutton" iconcls="icon-add" onclick="AddTypeGoods()">添加</a>
                <a href="#" class="easyui-linkbutton" iconcls="icon-save" onclick="BatchSaveDg()">保存</a>
                <a href="#" class="easyui-linkbutton" iconcls="icon-no" onclick="DeleteTypeInfoById()">删除</a>

            </div>
        </div>

        <div id="editWindows" title="信息编辑栏" data-options="iconCls:'icon-edit'" style="height:550px; width:500px; padding:10px;background:#fafafa;">
            <form method="post">
                <input type="hidden" id="dotype" />

            </form>
        </div>

    </div>
    <div data-options="region:'east',title:'查询条件',split:true" style="width:300px;">

    </div>

    <script src="~/Scripts/myJs/jfBcParamSetting.js"></script>

</body>
</html>
