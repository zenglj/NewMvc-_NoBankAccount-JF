
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width">
    <title>商品信息</title>
    @{Html.RenderPartial("~/Views/Shared/_PartialHeadPage.cshtml");}

    <link href="~/Content/outzhenComStyle.css" rel="stylesheet" />

    <script src="~/Scripts/myJs/commonJs.js"></script>
    <script src="~/Scripts/myJs/EditGoods.js"></script>
</head>
<body class="easyui-layout">
    <div data-options="region:'west',title:'商品管理',split:true" style="width:800px;">
        <div style="height:250px;">
            <table id="test" class="easyui-datagrid"></table>
        </div>
        <div id="editWindows" title="信息编辑栏" data-options="iconCls:'icon-edit'" style="height:350px; width:300px; padding:10px;background:#fafafa;">
            <form method="post">
                <input type="hidden" id="dotype" />
                <table>
                    <tr>
                        <th align="right">编号：</th>
                        <td><input disabled="disabled" id="GCODE" style="width:130px;" /></td>
                    </tr>
                    <tr>
                        <th align="right">名称：</th>
                        <td><input id="GNAME" style="width:130px;" /></td>
                    </tr>
                    <tr>
                        <th align="right">类别：</th>
                        <td>
                            <input id="FGtype" name="FGtype" class="easyui-validatebox" data-options="required:true" value="请选择类别" style="width:135px" />
                        </td>
                        </td>
                    </tr>
                    <tr>
                        <th align="right">单位：</th>
                        <td><input id="FUnit" style="width:130px;" /></td>
                    </tr>
                    <tr>
                        <th align="right">规格：</th>
                        <td><input id="FStandard" name="FStandard" style="width:130px" /></td>
                    </tr>
                    <tr>
                        <th align="right">单价：</th>
                        <td><input id="FPrice" disabled="disabled" class="easyui-numberbox" style="width:130px;" data-options="min:0,precision:2" /></td>
                    </tr>
                    <tr>
                        <th align="right">供应商：</th>
                        <td><input id="FSupplyer" name="FSupplyer" class="easyui-validatebox" data-options="required:true" value="请选择商家" style="width:135px" /></td>
                    </tr>
                    <tr>
                        <th align="right">条形码：</th>
                        <td><input id="FGTXM" class="easyui-numberbox" style="width:130px;" data-options="min:0,precision:0" /></td>
                    </tr>
                    <tr>
                        <th align="right">产地：</th>
                        <td colspan="7"><input id="FMadein" style="width:130px;" /></td>
                    </tr>
                    <tr>
                        <th align="right">非限额商品：</th>
                        <td>
                            <input type="checkbox" id="Ffreeflag" checked="checked" />
                        </td>
                    </tr>
                    <tr>
                        <th></th>
                        <td> <a href="#" id="btnSave" class="easyui-linkbutton" data-options="iconCls:'icon-save'" onclick="saveContent()">保存</a></td>
                    </tr>

                </table>
            </form>
        </div>

    </div>
    <div data-options="region:'center',title:'查询条件'" style="padding:5px;">
        <div data-options="region:'north',title:'North Title',split:true" style="height:200px;">
            <form method="post">
                <table id="crimeSearch">
                    <tr>
                        <th align="right">商品名称：</th>
                        <td align="left">
                            <input id="FGoodsName" name="FGoodsName" class="easyui-validatebox" style="width:190px;" data-options="required:true,validType:'text'" />
                        </td>
                    </tr>
                    <tr>
                        <th align="right">条形码：</th>
                        <td align="left">
                            <input id="FGoodsGTXM" name="FGoodsGTXM" class="easyui-validatebox" style="width:190px;" data-options="required:true,validType:'text'" />
                        </td>
                    </tr>
                    <tr>
                        <th align="right">商品类别：</th>
                        <td align="left">
                            <input id="FGoodsType" name="FGoodsType" value="请选择类别" style="width:190px;" />
                        </td>
                    </tr>
                    <tr>
                        <th align="right">商品状态：</th>
                        <td align="left">
                            <input id="FGoodsStatus" name="FGoodsStatus" value="请选择状态" style="width:190px;" />
                        </td>
                    </tr>
                    <tr>
                        <th align="right">是否限额：</th>
                        <td align="left">
                            <input id="FGoodsIsXianE" name="FGoodsIsXianE" value="请选择限额" style="width:190px;" />
                        </td>
                    </tr>
                    <tr>
                        <th align="right"></th>
                        <td align="right"><a id="btn" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-search'" onclick="FilterSearch()">查询</a><a id="A1" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-redo'" onclick="clearSearch()">清空</a></td>
                    </tr>
                </table>
            </form>
        </div>
    </div>

</body>
</html>
